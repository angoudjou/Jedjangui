@model IEnumerable<JedjanguiWeb.Models.InscrisExercice>

@{
    ViewBag.Title = JedjanguiWeb.Resources.Layout.Layout.TitreAjouter;
}

<h2>Inscris</h2>

<p>
    @Html.ActionLink(JedjanguiWeb.Resources.Layout.Layout.TitreAjouter, "InscrireMembre")
</p>
<table class=" table table-bordered table-striped table-responsive table-hover table-condensed">
    <tr>
        <th>
            ID
        </th>
        <th>
            Nom
        </th>
        <th>
            Responsabilité
        </th>

    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CODEMEMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MEMBRE.NOMMEMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.POSTEMEMBREEXO)
        </td>
        <td>
            @Html.ActionLink("Edit", "Incrire", new { id=item.CODEINSCRIPTIONEXERCICE }) |
           
            @Html.ActionLink("Delete", "Delete", new { id=item.CODEINSCRIPTIONEXERCICE })
        </td>
    </tr>
}

</table>
