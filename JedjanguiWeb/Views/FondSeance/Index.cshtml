@model IEnumerable<JedjanguiWeb.Models.FondSeance>

@{
    ViewBag.Title = JedjanguiWeb.Resources.Layout.Layout.TitreFond;
}

<h2>Index</h2>

<p>
    @Html.ActionLink(JedjanguiWeb.Resources.Layout.Layout.TitreAjouter, "Create")
</p>
<table class="table table-bordered table-striped table-responsive table-hover table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CODEFONDSEANCE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FOND.NOMFOND)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CREDITFONDSEANCE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DEBITFONDSEANCE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CODEFONDSEANCE)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.FOND.NOMFOND)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DEBITFONDSEANCE)

        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MONTANTCOTISATION)

        </td>
        <td>
            @Html.ActionLink("Caluculer Interet", "CaluculerInteret", new { id = item.CODEFONDSEANCE }) |
            @Html.ActionLink("Edit", "Edit", new { id = item.CODEFONDSEANCE }) |
            @Html.ActionLink("Details", "Details", new { id = item.CODEFONDSEANCE }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.CODEFONDSEANCE })
        </td>
    </tr>
}

</table>
